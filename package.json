{
  "name": "cdk_app_template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "^1.5.5"
  },
  "engines": {
    "node": "16.17.1",
    "yarn": "3.2.0"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@types/aws-lambda": "^8.10.106",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "husky": "^8.0.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.8.4"
  }
}
