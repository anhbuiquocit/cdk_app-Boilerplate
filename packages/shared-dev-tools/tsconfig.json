{
  "compilerOptions": {
    "lib": ["es2018", "esnext.asynciterable"],
    "baseUrl": "./",
    "moduleResolution": "node",
    "target": "es2018",
    "module": "commonjs",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noImplicitAny": true
  }
}
